{
  "expo": {
    "name": "LoaEscuela",
    "slug": "loaescuela",
    
    "platforms": ["ios", "android"],


    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoloa.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logoloa.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFB421"
    },
    "ios": {
      "bundleIdentifier": "com.loasurf.loaescuela",
      "buildNumber": "1.0.0",
      "icon": "./assets/logoloa.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.loasurf.loaescuela",
      "versionCode": 1,
      "icon": "./assets/icon.png",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-asset",
      "react-native-edge-to-edge"
    ],
    "extra": {
      "eas": {
        "projectId": "9b36f4ee-3997-49a6-8646-0df184216412"
      }
    }
  }
}
